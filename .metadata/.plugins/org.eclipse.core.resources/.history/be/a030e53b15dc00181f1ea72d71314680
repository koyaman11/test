	public int itemListHistryDelete() throws SQLException{
		String sql="DELETE FROM item_info_transaction WHERE";
		PreparedStatement preparedStatement;
		int result=0;
		try{
			preparedStatement=connection.prepareStatement(sql);
			result=preparedStatement.executeUpdate();
		}catch(SQLException e){
			e.printStackTrace();
		}fanally{
			connection.close();
		}
				return result;
